{
  "compilerOptions": {
    "baseUrl": "./src/",
    "outDir": "./public/",
    "module": "commonjs",
    "target": "es5",
    "lib": ["es2015", "es2017.object", "dom"],
    "jsx": "react",
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "importHelpers": true,
    "sourceMap": true
  }
}
